{
    "manifest_version": 3,
    "name": "Fallen London Sell All",
    "version": "1.0.0",
    "description": "Adds added funtionality to the Bazaar page for easier selling.",
    "content_scripts": [
      {
        "matches": [
          "https://www.fallenlondon.com/*"
        ],
        "js": [
          "sellall.js"
        ]
        
      }
    ],
    "permissions": [
      "activeTab",
      "declarativeContent"
    ]
  }